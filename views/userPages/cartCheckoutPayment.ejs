  <script>
    async function checkoutReview() {
      let paymentMethod1 = document.getElementById('paymentMethod1');
      let paymentMethod3 = document.getElementById('paymentMethod3');
      let paymentMethod2 = document.getElementById('paymentMethod2');
      // let paymentMethod4 = document.getElementById('paymentMethod4');
      let paymentMethodValue;

      if (paymentMethod1.checked) {
        paymentMethodValue = paymentMethod1.value;
      } else if (paymentMethod3.checked) {
        paymentMethodValue = paymentMethod3.value;
      } else if (paymentMethod2.checked) {
        paymentMethodValue = paymentMethod2.value;
      }
      else {
        paymentMethodValue = null;
      }

      let response = await fetch(`/cartCheckout/review?id=${paymentMethodValue}`, {
        method: 'GET'
      })
      let result = await response.json()
      if (result.success) {
        window.location.href = '/orderSummary'
      } if (result.noPaymentMethod) {
        await Swal.fire({
          icon: "error",
          title: "Kindly select payment method"
        })
      }if(result.codLimit){
        await Swal.fire({
          icon: "error",
          title: "COD not available for purchases above $1000"
        })
      }if(result.walletBalanceZero){
        await Swal.fire({
          icon: "error",
          title: "Your wallet balance: 0.",
          text: "Please select other payment options."
        })
      }if(result.noWalletBalance){
        await Swal.fire({
          icon: "error",
          title: "check wallet balance",
          text: "Please select other payment options."
        })
      }
    }
  </script>

