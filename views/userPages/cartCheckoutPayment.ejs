                <div class="col-sm-12">
                  <div class="well well-lg clearfix">
                    <ul class="pager">
                      <li class="next">
                        <a class='btn btn-primary btn-default float-end' onclick="checkoutReview()">Continue <i
                            class="fa fa-angle-right"></i></a>
                      </li>
                    </ul>
                  </div>
                </div>
              </div>
            </form>



          </div>
